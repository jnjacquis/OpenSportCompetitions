source 'https://github/CocoaPods/Specs.git

# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'OpenSportCompetitions' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OpenSportCompetitions
  pod 'Swinject', '~> 2.1.0'
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'

  target 'OpenSportCompetitionsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'OpenSportCompetitionsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
